{
    "name": "origami/consent",
    "description": "Helper interface for user consent in laravel projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank",
            "email": "david@papertank.co.uk"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Consent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Origami\\Consent\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Consent\\ConsentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
